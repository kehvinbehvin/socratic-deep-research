  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "serverless offline start",
    "dev": "docker compose up",
    "dev:build": "docker compose up --build",
    "dev:clean": "docker compose down -v",
    "db:up": "docker compose up postgres -d",
    "db:down": "docker compose stop postgres",
    "db:clean": "docker compose down postgres -v",
    "watch": "tsc -w",
    "start:local": "npm run db:clean && npm run db:up && sleep 5 && npm run build && npm run start",
    "start:fresh": "npm run db:clean && npm run dev:build"
  },
